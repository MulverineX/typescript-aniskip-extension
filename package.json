{
  "name": "typescript-opening-skipper-frontend",
  "extensionName": "Opening and Ending Skipper",
  "version": "0.4.0",
  "description": "An extension which gives the option to skip opening and endings of media on various streaming sites",
  "main": "content.ts",
  "repository": "https://github.com/lexesjan/typescript-opening-skipper-frontend",
  "author": "Lexes Jan Mantiquilla <mantiquillal@gmail.com>, Dabreadman <tany3@tcd.ie>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.config.js",
    "build:chrome": "cross-env NODE_ENV=production BROWSER=chromium webpack --config ./webpack/webpack.prod.config.js",
    "build:firefox": "cross-env NODE_ENV=production BROWSER=firefox webpack --config ./webpack/webpack.prod.config.js",
    "dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.dev.config.js --watch",
    "dev:chrome": "cross-env NODE_ENV=development BROWSER=chromium webpack --config ./webpack/webpack.dev.config.js --watch",
    "dev:firefox": "cross-env NODE_ENV=development BROWSER=firefox webpack --config ./webpack/webpack.dev.config.js --watch"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.128",
    "@types/classnames": "^2.2.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.0",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "autoprefixer": "^10.2.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.2",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.1.0",
    "mini-css-extract-plugin": "^1.3.6",
    "postcss": "^8.2.5",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^5.0.0",
    "prettier": "^2.2.1",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.6",
    "sass-loader": "^11.0.1",
    "schema-utils": "^3.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "tailwindcss": "^2.0.3",
    "uuid": "^8.3.2",
    "webextension-polyfill-ts": "^0.24.0"
  }
}
