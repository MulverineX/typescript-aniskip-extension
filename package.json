{
  "name": "typescript-opening-skipper-frontend",
  "extensionName": "Opening and Ending Skipper",
  "version": "0.3.0",
  "description": "An extension which gives the option to skip opening and endings of media on various streaming sites",
  "main": "content.ts",
  "repository": "https://github.com/lexesjan/typescript-opening-skipper-frontend",
  "author": "Lexes Jan Mantiquilla <mantiquillal@gmail.com>, Dabreadman <tany3@tcd.ie>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "webpack --env NODE_ENV=production --config ./webpack/webpack.prod.config.js",
    "build:chrome": "webpack --env NODE_ENV=production --env BROWSER=chromium --config ./webpack/webpack.prod.config.js",
    "build:firefox": "webpack --env NODE_ENV=production --env BROWSER=firefox --config ./webpack/webpack.prod.config.js",
    "dev": "webpack --env NODE_ENV=development --config ./webpack/webpack.dev.config.js --watch",
    "dev:chrome": "webpack --env NODE_ENV=development --env BROWSER=chromium --config ./webpack/webpack.dev.config.js --watch",
    "dev:firefox": "webpack --env NODE_ENV=development --env BROWSER=firefox --config ./webpack/webpack.dev.config.js --watch"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.128",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.0",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.2",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.6",
    "sass-loader": "^11.0.1",
    "schema-utils": "^3.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "uuid": "^8.3.2"
  }
}
