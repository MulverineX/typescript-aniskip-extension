{
  "name": "typescript-opening-skipper-frontend",
  "extensionName": "Opening and Ending Skipper",
  "version": "0.7.0",
  "description": "An extension which gives the option to skip opening and endings of media on various streaming sites",
  "main": "content.ts",
  "repository": "https://github.com/lexesjan/typescript-opening-skipper-frontend",
  "contributors": [
    "Lexes Jan Mantiquilla <mantiquillal@gmail.com>",
    "Dabreadman <tany3@tcd.ie>"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.config.js",
    "build:chrome": "cross-env NODE_ENV=production BROWSER=chromium webpack --config ./webpack/webpack.prod.config.js",
    "build:firefox": "cross-env NODE_ENV=production BROWSER=firefox webpack --config ./webpack/webpack.prod.config.js",
    "dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.dev.config.js --watch",
    "dev:chrome": "cross-env NODE_ENV=development BROWSER=chromium webpack --config ./webpack/webpack.dev.config.js --watch",
    "dev:firefox": "cross-env NODE_ENV=development BROWSER=firefox webpack --config ./webpack/webpack.dev.config.js --watch"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.134",
    "@types/classnames": "^2.2.11",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/string-similarity": "^4.0.0",
    "@types/uuid": "^8.3.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "autoprefixer": "^10.2.5",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.1",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.1",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.4.1",
    "postcss": "^8.2.9",
    "postcss-import": "^14.0.1",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "schema-utils": "^3.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "string-similarity": "^4.0.4",
    "tailwindcss": "^2.1.1",
    "uuid": "^8.3.2",
    "webextension-polyfill-ts": "^0.25.0"
  }
}
